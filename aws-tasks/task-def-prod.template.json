{
  "family": "bi-app-net-scanner-prod",
  "taskRoleArn": "arn:aws:iam::475636345445:role/ECSTaskRole",
  "containerDefinitions": [{
      "name": "dotnet",
      "image": "{{WILL_BE_FILLED_DYNAMICALLY}}",
      "memoryReservation": 128,
      "essential": true,
      "portMappings": [{
        "hostPort": 700,
        "containerPort": 80,
        "protocol": "tcp"
      }],
      "environment": [{
        "name": "ASPNETCORE_ENVIRONMENT",
        "value": "Production"
      }],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/api-prod-cluster/bi-app-net-scanner-prod",
          "awslogs-region": "ap-northeast-2"
        }
      },
      "dockerLabels": {
        "traefik.enable": "true",
        "traefik.frontend.rule": "Host:bi-app-net-scanner-prod.bincentive.com",
        "traefik.frontend.entryPoints": "public-http,public-https"
      }
    }
 ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "bridge"
}
